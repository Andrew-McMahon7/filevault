{
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/my-application/*.log",
            "log_group_name": "/aws/eks/filevault-eks-${local.environment}/application",
            "log_stream_name": "{instance_id}"
          }
        ]
      }
    }
  }
}